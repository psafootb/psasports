<h1>Listing teams</h1>

<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>City</th>
    <th>State</th>
    <th>Organization</th>
    <th>Sport</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.user_id %></td>
    <td><%= team.name %></td>
    <td><%= team.city %></td>
    <td><%= team.state %></td>
    <td><%= team.organization %></td>
    <td><%= team.sport %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
